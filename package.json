    "pkgcloud": "~0.8.12",
    "less": "~1.4.2",
    "jade": "~0.35.0",
    "mime": "1.2.11",
    "nib": "~1.0.1",
    "underscore": "~1.5.2",
    "coffee-script": "~1.6.3",
    "markdown": "~0.5.0",
